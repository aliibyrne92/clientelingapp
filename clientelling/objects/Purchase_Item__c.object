    <fields>
        <fullName>Amount__c</fullName>
        <description>Retail IDO Custom Field</description>
        <externalId>false</externalId>
        <label>Price</label>
        <precision>8</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Image__c</fullName>
        <description>Retail IDO Custom Field</description>
        <externalId>false</externalId>
        <formula>Image(Product__r.Image_URL__c, &quot;Product&quot;, 50,50)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Image</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Product__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Retail IDO Custom Field</description>
        <externalId>false</externalId>
        <label>Product</label>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>Purchase Items</relationshipLabel>
        <relationshipName>Purchase_Itemsab</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
